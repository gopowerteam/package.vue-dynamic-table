{
  "name": "package.vue-dynamic-table",
  "version": "1.0.0",
  "private": true,
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "lerna run build",
    "test": "lerna run test",
    "dev": "lerna run dev --scope playground --include-dependencies",
    "lint": "lerna run lint",
    "publish:patch": "lerna publish patch --no-private --yes",
    "publish:minor": "lerna publish minor --no-private --yes",
    "publish:major": "lerna publish major --no-private --yes",
    "release": "npm run build && npm run publish:patch",
    "commit": "git-cz",
    "lint:eslint": "eslint \"src/**/*.{vue,ts,tsx}\" --fix",
    "precommit": "chmod +x .husky/*"
  },
  "workspaces": [
    "playground/*",
    "packages/*"
  ],
  "devDependencies": {
    "lerna": "^6.6.1",
    "@babel/core": "7.21.3",
    "@babel/eslint-parser": "7.21.3",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/node": "18",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "commitizen": "4.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "latest",
    "rimraf": "4.4.1",
    "typescript": "5.0.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.11.0",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
