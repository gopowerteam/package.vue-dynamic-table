{
  "name": "@gopowerteam/vue-dynamic-table",
  "version": "0.0.83",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "files": [
    "dist",
    "resolver.d.ts",
    "README.md"
  ],
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./resolver": {
      "require": "./dist/lib/resolver.js",
      "import": "./dist/es/resolver.mjs",
      "types": "./dist/es/resolver.d.ts"
    },
    "./dist/style.css": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@arco-design/web-vue": "2.50.2",
    "@gopowerteam/vue-modal": "~0.1.17",
    "dayjs": "1.11.9",
    "exceljs": "^4.3.0",
    "mitt": "3.0.1",
    "vue": "3.3.4",
    "vxe-table": "4.5.5",
    "xe-utils": "3.5.12"
  },
  "peerDependencies": {
    "vue": "3.2.0"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.5",
    "@rushstack/eslint-patch": "1.3.3",
    "@types/jsdom": "21.1.2",
    "@types/node": "20.5.6",
    "@vitejs/plugin-vue": "4.3.3",
    "@vitejs/plugin-vue-jsx": "3.0.2",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "2.4.1",
    "@vue/tsconfig": "0.4.0",
    "eslint": "8.47.0",
    "eslint-plugin-vue": "9.17.0",
    "jsdom": "22.1.0",
    "prettier": "3.0.2",
    "typescript": "~5.2.2",
    "unplugin-vue-components": "^0.25.1",
    "vite": "4.4.9",
    "vite-plugin-dts": "3.5.2",
    "vitest": "0.34.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gopowerteam/package.vue-dynamic-table.git"
  },
  "bugs": {
    "url": "https://github.com/gopowerteam/package.vue-dynamic-table/issues"
  },
  "homepage": "https://github.com/gopowerteam/package.vue-dynamic-table",
  "license": "MIT"
}
