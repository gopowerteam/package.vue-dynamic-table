{
  "name": "@gopowerteam/vue-dynamic-table",
  "version": "0.0.64",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "files": [
    "dist",
    "resolver.d.ts",
    "README.md"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      }
    },
    "./resolver": {
      "require": "./dist/resolver.cjs.js",
      "import": "./dist/resolver.es.js",
      "types": "./dist/resolver.d.ts"
    },
    "./dist/style.css": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    }
  },
  "dependencies": {
    "@gopowerteam/vue-modal": "~0.1.15",
    "dayjs": "1.11.7",
    "exceljs": "^4.3.0",
    "mitt": "3.0.0",
    "vue": "3.2.45",
    "vue-json-viewer": "3",
    "vxe-table": "4.3.10",
    "xe-utils": "3.5.7"
  },
  "peerDependencies": {
    "vue": "3.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.18",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.2.7",
    "@vue/tsconfig": "0.1.3",
    "eslint": "8.31.0",
    "eslint-plugin-vue": "9.8.0",
    "jsdom": "21.0.0",
    "prettier": "2.8.2",
    "typescript": "~4.9.4",
    "vite": "4.0.4",
    "vite-plugin-dts": "1.7.1",
    "vitest": "0.27.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gopowerteam/package.vue-dynamic-table.git"
  },
  "bugs": {
    "url": "https://github.com/gopowerteam/package.vue-dynamic-table/issues"
  },
  "homepage": "https://github.com/gopowerteam/package.vue-dynamic-table",
  "license": "MIT"
}
